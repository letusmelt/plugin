{
  "manifest_version": 3,
  "name": "Happy Christmas Emoji Decorator",
  "version": "1.0",
  "description": "decorate your web page with happy christmas emojis",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_title": "Happy Christmas Emoji Decorator"
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
} 